<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LeftMenuButtonsController">
    <VBox fx:id="flowPane" alignment="CENTER" maxWidth="100" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">

    </VBox>
    <Button fx:id="addButton" contentDisplay="BOTTOM" onAction="#addBuildingLevel" styleClass="buttonConfirm" stylesheets="@../stylesheets/confirm.css" text="%mw.button.levels" textAlignment="CENTER" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
      <VBox.margin>
         <Insets />
      </VBox.margin>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
      <effect>
         <DropShadow color="GREY" />
      </effect></Button>
</VBox>
